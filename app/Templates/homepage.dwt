<!-- =callback -->
<div class="modal fade" id="callback" tabindex="-1" role="dialog" aria-hidden="true">
	<div class="modal-dialog modal-dialog-centered" role="document">
		<div class="modal-container">
			<button type="button" data-dismiss="modal" class="close-modal" tabindex="10">
				<svg width="17" height="17">
					<line x1="0" y1="0" x2="17" y2="17"/>
					<line x1="17" y1="0" x2="0" y2="17"/>
				</svg>
			</button>
            
			<form action="#" method="post" class="form-policy" id="callback-form">
				<fieldset>
					<label for="callback-name" class="visually-hidden">как вас зовут</label>
				    <input type="text" name="name" id="callback-name" class="form-control required" value="" tabindex="0" placeholder="как вас зовут" autofocus /><span class="error"></span>

				    <label for="callback-tel" class="visually-hidden">ваш телефон</label>
					<input type="tel" name="tel" id="callback-tel" class="form-control required tel" value="" tabindex="1" placeholder="Ваш телефон" /><span class="error"></span>

					<input type="submit" name="callback_submit" class="submit" tabindex="2" value="отправить" />
					<div class="policy">
						<input type="checkbox" id="checkbox1" value="" name="checkbox" checked="checked" class="user-access" tabindex="4" />
						<label for="checkbox1">Я Согласен на обработку персональных данных и принимаю условия <a href="#.pdf" title="" target="_blank">Пользовательского соглашения</a></label>
					</div>
